<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.mapper.normalLogin">
  <select id="login" useCache="true" parameterType="com.pojo.userData" 
  			resultType="com.pojo.userData" >
  	select username,password,roleid,depid from RUser where username= '${username}'
  </select>
</mapper>
